# %%
df.head()

# %%
df.shape

# %%
df['OBS_VALUE'].describe

# %% [markdown]
# # Public Sector Net-worth(Mean)

# %% [markdown]
# ## 1. Austria Public Sector Net Worth (Mean)

# %%
mean_networthAT18 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2018-Q1') & 
    (df['TIME_PERIOD'] <= '2018-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT18)

mean_networthAT19 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2019-Q1') & 
    (df['TIME_PERIOD'] <= '2019-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT19)

mean_networthAT20 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2020-Q1') & 
    (df['TIME_PERIOD'] <= '2020-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT20)

mean_networthAT21 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2021-Q1') & 
    (df['TIME_PERIOD'] <= '2021-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT21)

mean_networthAT22 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2022-Q1') & 
    (df['TIME_PERIOD'] <= '2022-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT22)

mean_networthAT23 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2023-Q1') & 
    (df['TIME_PERIOD'] <= '2023-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT23)

mean_networthAT24 = df.loc[
    (df['geo']=='AT') & 
    (df['TIME_PERIOD'] >= '2021-Q1') & 
    (df['TIME_PERIOD'] <= '2021-Q4'),
    'OBS_VALUE'
].mean()
print(mean_networthAT24)


