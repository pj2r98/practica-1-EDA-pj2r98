# %% [markdown]
# ## New Data Frame

# %%
import pandas as pd
countries = [Slovakia, Slovenia, Romania, Malta, Spain, France, Germany, Portugal, Italy, Croatia, Hungary, Poland, Czechia, Slovakia, Slovenia, Greece, Bulgaria, Sweden, Denmark, Norway, Austria,Luxembourg, Lithuania, Estonia, Latvia, Ireland, Belgium, Netherlands, Finland, Cyprus ]
ndf_list = []
for c in countries:
    ndf = pd.DataFrame(c)
    ndf_list.append(ndf)
final_ndf = pd.concat(ndf_list, ignore_index=True)

data_frame = pd.DataFrame(final_ndf)
print(data_frame)

# %%
final_ndf2018 = final_ndf[final_ndf['Year'] == 2018]
final_ndf2019 = final_ndf[final_ndf['Year']== 2019]
final_ndf2020 = final_ndf[final_ndf['Year'] == 2020]
final_ndf2021 = final_ndf[final_ndf['Year'] == 2021]
final_ndf2022 = final_ndf[final_ndf['Year'] == 2022]
final_ndf2023 = final_ndf[final_ndf['Year'] == 2023]
final_ndf2024 = final_ndf[final_ndf['Year'] == 2024]
fig, ax = plt.subplots(figsize=(10, 5))

sns.scatterplot(
    data=(final_ndf2020),
    x='PSNW',
    y='Real GDP Growth Rate',
    ax=ax
)

ax.grid()

# plt.tight_layout()

plt.show()

# %%
Germany= data_frame[data_frame['Country'] == 'Germany']
plt.figure()
plt.scatter(Germany['Year'], Germany['Real GDP Growth Rate'])
plt.xlabel('Year')
plt.ylabel('Real GDP Growth')
plt.title('Germany GDP Growth by Year')
plt.show()

# %%
plt.figure()
plt.scatter(Germany['Year'], Germany['PSNW'])
plt.xlabel('Year')
plt.ylabel('GDP Growth')
plt.title('Germany GDP Growth by Year')
plt.show()

# %%
data_frame.to_csv("PSNW_EU_GDP_GR", index=False)

# %%
df_year = data_frame[data_frame['Year'] == 2024]

plt.figure(figsize=(10, 6))
plt.bar(df_year['Country'], df_year['PSNW'])
plt.xlabel('Country')
plt.ylabel('Public Sector Net Worth (Millions of Euros)')
plt.title(f'GDP Growth by Country in {2024}')
plt.xticks(rotation=45)
plt.tight_layout()
plt.show()

